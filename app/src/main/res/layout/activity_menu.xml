<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color6"
        android:gravity="center"
        android:orientation="vertical"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <TextView
            android:id="@+id/tv_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/calistoga"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="35sp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="60dp" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="167dp"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_menu"
            android:contentDescription="@string/BTNtienda"
            android:drawableTop="@drawable/barril"
            android:onClick="tienda"
            android:padding="8dp"
            android:src="@drawable/barril"
            android:text="@string/BTNtienda"
            tools:ignore="ImageContrastCheck" />
        <TextView
            android:id="@+id/textView4"
            android:layout_width="167dp"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_menu"
            android:fontFamily="@font/calistoga"
            android:text="@string/BTNtienda"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="15dp" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="167dp"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_menu"
            android:contentDescription="@string/BTNhorario"
            android:drawableTop="@drawable/calendario"
            android:onClick="horario"
            android:padding="8dp"
            android:src="@drawable/calendario"
            android:text="@string/BTNhorario"
            tools:ignore="ImageContrastCheck" />
        <TextView
            android:id="@+id/textView3"
            android:layout_width="167dp"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_menu"
            android:fontFamily="@font/calistoga"
            android:text="@string/BTNhorario"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="15dp" />


        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="167dp"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_menu"
            android:contentDescription="@string/BTNcontacto"
            android:drawableTop="@drawable/calendario"
            android:onClick="contacto"
            android:padding="8dp"
            android:src="@drawable/comunicar"
            android:text="@string/BTNcontacto"
            tools:ignore="ImageContrastCheck" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="167dp"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_menu"
            android:fontFamily="@font/calistoga"
            android:text="@string/BTNcontacto"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
